{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Talk",
	"type": "object",
	"required": ["title", "description", "event"],
	"properties": {
		"title": {
			"type": "string"
		},
		"event": {
			"type": "string"
		},
		"watch": {
			"type": "string",
			"format": "uri"
		},
		"description": {
			"type": "string"
		},
		"slides": {
			"type": "string",
			"format": "uri"
		},
		"date": {
			"type": "string",
			"format": "date"
		}
	}
}
